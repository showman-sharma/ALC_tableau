<KB>
    <Class>
        <CONCEPT>NNF1</CONCEPT>
        <EquivalentTo>
            <OR>
                <CONCEPT>Mammal</CONCEPT>
                <AND>
                    <NOT>
                        <CONCEPT>Person</CONCEPT>
                    </NOT>
                    <CONCEPT>Biped</CONCEPT>
                </AND>
            </OR>
        </EquivalentTo>
    </Class>
    <Class>
        <CONCEPT>NNF2</CONCEPT>
        <EquivalentTo>
            <AND>
                <OR>
                    <NOT>
                        <CONCEPT>Woman</CONCEPT>
                    </NOT>
                    <AND>
                        <CONCEPT>Person</CONCEPT>
                        <CONCEPT>Female</CONCEPT>
                    </AND>
                </OR>
                <OR>
                    <NOT>
                        <CONCEPT>Person</CONCEPT>
                    </NOT>
                    <NOT>
                        <CONCEPT>Female</CONCEPT>
                    </NOT>
                    <CONCEPT>Woman</CONCEPT>
                </OR>
            </AND>
        </EquivalentTo>
    </Class>
    <Class>
        <CONCEPT>NNF3</CONCEPT>
        <EquivalentTo>
            <AND>
                <OR>
                    <NOT>
                        <CONCEPT>Man</CONCEPT>
                    </NOT>
                    <AND>
                        <CONCEPT>Person</CONCEPT>
                        <CONCEPT>Male</CONCEPT>
                    </AND>
                </OR>
                <OR>
                    <NOT>
                        <CONCEPT>Person</CONCEPT>
                    </NOT>
                    <NOT>
                        <CONCEPT>Male</CONCEPT>
                    </NOT>
                    <CONCEPT>Man</CONCEPT>
                </OR>
            </AND>
        </EquivalentTo>
    </Class>
    <Class>
        <CONCEPT>NNF4</CONCEPT>
        <EquivalentTo>
            <AND>
                <OR>
                    <NOT>
                        <CONCEPT>Father</CONCEPT>
                    </NOT>
                    <AND>
                        <CONCEPT>Man</CONCEPT>
                        <EXISTS>
                            <ROLE>parentOf</ROLE>
                            <CONCEPT>Person</CONCEPT>
                        </EXISTS>
                    </AND>
                </OR>
                <OR>
                    <NOT>
                        <CONCEPT>Man</CONCEPT>
                    </NOT>
                    <FORALL>
                        <ROLE>parentOf</ROLE>
                        <NOT>
                            <CONCEPT>Person</CONCEPT>
                        </NOT>
                    </FORALL>
                    <CONCEPT>Father</CONCEPT>
                </OR>
            </AND>
        </EquivalentTo>
    </Class>
    <Class>
        <CONCEPT>NNF5</CONCEPT>
        <EquivalentTo>
            <AND>
                <OR>
                    <NOT>
                        <CONCEPT>Mother</CONCEPT>
                    </NOT>
                    <AND>
                        <CONCEPT>Woman</CONCEPT>
                        <EXISTS>
                            <ROLE>parentOf</ROLE>
                            <CONCEPT>Person</CONCEPT>
                        </EXISTS>
                    </AND>
                </OR>
                <OR>
                    <NOT>
                        <CONCEPT>Woman</CONCEPT>
                    </NOT>
                    <FORALL>
                        <ROLE>parentOf</ROLE>
                        <NOT>
                            <CONCEPT>Person</CONCEPT>
                        </NOT>
                    </FORALL>
                    <CONCEPT>Mother</CONCEPT>
                </OR>
            </AND>
        </EquivalentTo>
    </Class>
    <Class>
        <CONCEPT>NNF6</CONCEPT>
        <EquivalentTo>
            <AND>
                <OR>
                    <NOT>
                        <CONCEPT>Bachelor</CONCEPT>
                    </NOT>
                    <AND>
                        <CONCEPT>Man</CONCEPT>
                        <FORALL>
                            <ROLE>husbandOf</ROLE>
                            <NOT>
                                <CONCEPT>Person</CONCEPT>
                            </NOT>
                        </FORALL>
                    </AND>
                </OR>
                <OR>
                    <NOT>
                        <CONCEPT>Man</CONCEPT>
                    </NOT>
                    <EXISTS>
                        <ROLE>parentOf</ROLE>
                        <CONCEPT>Person</CONCEPT>
                    </EXISTS>
                    <CONCEPT>Bachelor</CONCEPT>
                </OR>
            </AND>
        </EquivalentTo>
    </Class>
</KB>
